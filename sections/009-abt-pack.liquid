<div class="em-pack-wrapper custom-space overflow-hidden">
    <div class="vessel">
        <div class="queue">
            <div class="column-12 column-lg-8">
                <div class="content-container margin-bottom">
                    <h2>{{ section.settings.title }}</h2>
                    <p>{{ section.settings.rich }}</p>
                </div>
            </div>
            <div class="em-cc-00 column-12 column-lg-4">
                <div class="image-wrap">
                    {% if section.settings.img %}
                    <img src="{{ section.settings.img | img_url: 'master' }}" alt="">
                    {% endif %}
                </div>
            </div>
        </div>
    </div>
</div>
{% schema %}
{
"name": "009-abt-pack",
"class": "ind-o2",
"settings": [
{
"type": "textarea",
"id": "title",
"label": "title",
"default": "MINDFUL Packaging"
},
{
"type": "richtext",
"id": "rich",
"label": "text long"
},
{
"type": "image_picker",
"id": "img",
"label": "img"
}
],
"blocks": [
],
"presets": [
{
"name": "009-abt-pack",
"category": "custom"
}
]
}
{% endschema %}