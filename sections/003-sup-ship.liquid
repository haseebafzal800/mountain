<div id="em-shipping" class="em-support-00 custom-space overflow-hidden">
    <div class="vessel">
        <div class="queue">
            <div class="column-12">
                <div class="cc-wrapper">
                    <div class="content-container margin-bottom">
                        <h2 class="text-left">{{ section.settings.title }}</h2>
                        <p>{{ section.settings.desc }}</p>
                    </div>
                    <div class="em-table-wrap">
                        <table class="table-wrapper margin-bottom">
                            <tbody>{{ section.settings.tbody }}
                            </tbody>
                        </table>
                    </div>
                    <div class="cc-container">
                        <p>
                            {{ section.settings.ship_txt }}
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
{% schema %}
{
"name": "003-sup-ship",
"settings": [
{
"type": "textarea",
"id": "title",
"label": "title",
"default": "Shipping"
},
{
"type": "textarea",
"id": "desc",
"label": "description"
},
{
"type": "textarea",
"id": "tbody",
"label": "tbody",
"default": "<tr> <th>Order Total</th> <th>USPS First Class (&#60;13 oz.) </th> <th>USPS Priority Mail</th> <th>Expedited FedEx 2Day</th> </tr> <tr> <td>Up to $20.00</td> <td>$3.75 - $6.15 (weight & zone)</td> <td>$7.37 + (weight & zone)</td> <td>$15.00 flat rate</td> </tr>"
},
{
"type": "textarea",
"id": "ship_txt",
"label": "ship_txt",
"default": "United States Postal Service Priority (USPS) Priority"
}
],
"blocks": [
],
"presets": [
{
"name": "003-sup-ship",
"category": "custom"
}
]
}
{% endschema %}